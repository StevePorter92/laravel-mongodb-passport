{
    "name": "steveporter92/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/steveporter92/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport"
    ],
    "require": {
        "illuminate/support": "^6.0",
        "jenssegers/mongodb": "^3.6",
        "laravel/passport": "^8.0",
        "illuminate/auth": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "StevePorter92\\Mongodb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StevePorter92\\Mongodb\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steve Porter",
            "email": "12199424+StevePorter92@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "StevePorter92\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
